<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AspectRatioImageView">
        <attr format="float" name="ari_ratio"/>
    </declare-styleable>



    <declare-styleable name="DesignSystemTag">
        <attr name="android:text" />
        <attr name="tagStyle" format="enum">
            <enum name="new_style" value="0" /> <!-- "new" is a reserved word apparently -->
            <enum name="preview" value="1" />
            <enum name="expiring" value="2" />
        </attr>
    </declare-styleable>

</resources>